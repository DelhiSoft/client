<?xml version='1.0' encoding='utf-8'?>
<widget id="com.delhisoft.spotchat" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Spot Chat</name>
    <description>
       Spot nearby people and chat. Find the perfect date near you.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
Spotchat and Gadgets Cure Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
